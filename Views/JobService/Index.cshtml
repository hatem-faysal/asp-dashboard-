@model testcrud.Data.ViewModels.JobServiceViewModel

@{

	ViewData["title"] = "index";
}
<div class="page-wrapper">
	<div class="page-content">
		<!--breadcrumb-->
		<div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
			<div class="breadcrumb-title pe-3">Tables</div>
			<div class="ps-3">
				<nav aria-label="breadcrumb">
					<ol class="breadcrumb mb-0 p-0">
						<li class="breadcrumb-item"><a href="javascript:;"><i class="bx bx-home-alt"></i></a>
						</li>
						<li class="breadcrumb-item active" aria-current="page">Data Table</li>
					</ol>
				</nav>
			</div>
		</div>
		<!--end breadcrumb-->
		<h6 class="mb-0 text-uppercase">Job Recruitment</h6>
		<hr />
		<div class="card">
			<div class="card-body">
				<div class="btn-group" >
					<a asp-action="Create" class="btn btn-primary mb-3 mb-lg-0"><i class="bx bxs-plus-square"></i>Create JobService</a>
				</div>
				<div class="table-responsive">
					<table id="employeeTable" class="yajra-datatable table table-striped table-bordered" style="width:100%">	
						<thead>
							<tr>
								<th>#</th>
								<th>Name</th>
								<th>CreatedAt</th>
								<th>Action</th>
							</tr>
						</thead>
					</table>
				</div>
			</div>
		</div>
	</div>
</div>

@section Scripts{
	<script src="~/js/datatable/JobServices.js"></script>
}