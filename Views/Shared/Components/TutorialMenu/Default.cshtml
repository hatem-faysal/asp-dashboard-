<!--breadcrumb-->
<div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
	@* <div class="breadcrumb-title pe-3">@ViewData["title"] ?? "Dashboard"</div> *@
	<div class="breadcrumb-title pe-3">
		    @((ViewData["title"] != null) ? ViewData["title"] : "book")
	</div>
	<div class="ps-3">
		<nav aria-label="breadcrumb">
			<ol class="breadcrumb mb-0 p-0">
				<li class="breadcrumb-item"><a asp-action="@ViewData["action"]"
						asp-controller="@ViewData["controller"]"><i class="bx bx-home-alt"></i></a>
				</li>
				<li class="breadcrumb-item active" aria-current="page">@ViewData["view"] </li>
			</ol>
		</nav>
	</div>
</div>
<!--end breadcrumb-->
<h6 class="mb-0 text-uppercase">@ViewData["category"] @ViewData["editValue"]</h6>
<hr />